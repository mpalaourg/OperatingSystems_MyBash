echo Outside myoutput ; echo Inside myoutput hello > myoutput && wc -l < myoutput
echo long ; echo hello > file_1 > file_2 > file_3 ; echo instruction
echo long && wc -l < file_1 < file_2 < file_3 && echo instruction
wc -l < file_3 > file_4
